    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "stopOnEntry": true,
            "pythonPath":"${config:python.pythonPath}",
            "program": "${file}",
            "cwd": "${workspaceFolder}",
            "env": {},
            "debugOptions": [
                "RedirectOutput"
            ]
        },
        {
            "name": "(lldb) yview",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/yview",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "lldb"
        },
        {
            "name": "(lldb) yitrace",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/yitrace",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "lldb"
        }
    ]
}